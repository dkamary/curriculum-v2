{% extends "base.html.twig" %}

{% set title = title is defined ? title : 'Page' %}

{% block title %}
    {{ title }}
{% endblock %}

{% block header %}
    {% include "_partials/section-header.html.twig" %}
{% endblock %}

{% block banner %}
    {% include "_partials/banner-page.html.twig" %}
{% endblock %}

{% block features %}{% endblock %}

{% block popular %}{% endblock %}

{% block featured_category %}{% endblock %}

{% block main_content %}
<section class="post-area section-gap">
    <div class="container">
        {% block content %}{% endblock %}
    </div>
</section>
{% endblock %}

{% block call2action %}{% endblock %}

{% block download %}{% endblock %}

{% block footer %}
    {% include "_partials/section-footer.html.twig" %}
{% endblock %}